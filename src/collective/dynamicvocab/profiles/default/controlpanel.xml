<?xml version="1.0"?>
<object name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="collective.dynamicvocab"
    purge="False">
    <configlet title="Dynamic Vocabularies"
        action_id="dynamicvocab"
        appId="collective.dynamicvocab"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/vocabularies"
        icon_expr="string:${portal_url}/++resource++collective.dynamicvocab/vocabulary_icon.png"
        visible="True"
        i18n:attributes="title">
    <permission>Manage portal</permission>
    </configlet>
</object>
